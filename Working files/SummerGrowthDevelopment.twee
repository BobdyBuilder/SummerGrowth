        }, this.createShadowWrapper(
                this.payload[0].contents !== ''
                 ? () => Wikifier.wikifyEval(this.payload[0].contents.trim())
                 : null,
                passage != null // lazy equality for null
                 ? () => Engine.play(passage)
                 : null))
